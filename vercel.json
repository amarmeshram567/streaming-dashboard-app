{
    "version": 3,
    "builds": [
        {
            "src": "next.config.js",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "NEXT_PUBLIC_TMDB_API_KEY": "@tmdb_api_key"
    }
}